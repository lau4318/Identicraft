{
  "name": "identicraft",
  "version": "1.0.0",
  "description": "Minecraft avatar rendering library, CLI, and serverless API",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./cli": "./bin/identicraft.js"
  },
  "bin": {
    "identicraft": "./bin/identicraft.js",
    "idc": "./bin/identicraft.js"
  },
  "scripts": {
    "start": "vercel dev",
    "deploy": "vercel --prod",
    "cli": "node bin/identicraft.js",
    "cli:test": "node bin/identicraft.js cube itsShiroharu -o cube.png",
    "prepublishOnly": "echo 'Ready to publish to NPM!'"
  },
  "keywords": [
    "javascript",
    "vercel",
    "nodejs",
    "serverless",
    "minecraft",
    "minecraft-api",
    "minecraft-skin",
    "avatar",
    "api",
    "cli",
    "skin",
    "render",
    "canvas",
    "image"
  ],
  "author": "Shengwei Xiong <contact@shiroharu.eu.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsShiroharu/Identicraft.git"
  },
  "bugs": {
    "url": "https://github.com/itsShiroharu/Identicraft/issues"
  },
  "homepage": "https://github.com/itsShiroharu/Identicraft#readme",
  "files": [
    "api/",
    "bin/",
    "lib/",
    "index.js",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@napi-rs/canvas": "^0.1.52",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "vercel": "^33.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}